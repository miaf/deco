<h1>Log in</h1>
<%= form_tag create_path, method: :post, id: 'log_in' do %>
	<p>
		<%= label_tag :login_name %><br/>
		<%= text_field_tag :login_name, params[:login_name] %>
	</p>
	<p>
		<%= label_tag :password %><br/>
		<%= password_field_tag :password %>
	</p>
	<%= submit_tag "Log in" %>
<% end %>